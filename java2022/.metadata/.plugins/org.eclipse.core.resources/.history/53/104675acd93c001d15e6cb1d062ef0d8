package classesWithAttributes;

public class Product {
	// attribute | field
	// this: İçerisinde bulunduğum claas demektir.
	// get: Getir anlamındadır. set: Planla anlamındadır.
	private int id;
	private String name;
	private String description;
	private double price;
	private int stockAmount;
	private String renk;
	private String kod;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

}
