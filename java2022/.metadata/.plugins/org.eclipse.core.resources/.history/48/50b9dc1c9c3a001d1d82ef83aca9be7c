
public class Main {

	public static void main(String[] args) {

		String[] araba = new String[15];

		araba[0] = "BMW";
		araba[1] = "MERCEDES";
		araba[2] = "AUDI";
		araba[3] = "VOLKSWAGEN";
		araba[4] = "OPEL";

		for (int i = 0; i < araba.length; i++) {
			System.out.println(araba[i]);
		}

	}

}
